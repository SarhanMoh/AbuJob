    "orientation": "portrait",
    "icon": "./app/assets/logo_1024.png",
    "splash": {
      "image": "./app/assets/logo_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfacestyle": "light",
    "updates": {
